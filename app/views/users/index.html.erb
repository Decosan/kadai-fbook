<h1>Users index</h1>
<ul class="media-list">
  <% @users.each do |user| %>
  	<li class="media">
  		<a class="media-left">
  			<h4>USER:</h4>
  		</a>
  		<div class="media-body">
  			<h4 class="media-heading"><%= user.name %></h4>
  			Register at: <%= user.created_at %>
  		</div>
  	</li>
  <% end %>
</ul>
